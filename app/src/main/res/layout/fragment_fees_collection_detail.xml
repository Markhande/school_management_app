<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".Fragments.FeesCollectionDetailFragment">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@drawable/heading_background">

            <TextView
                android:layout_weight="1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Fees"
                android:textColor="@color/heading_text_color"
                android:textSize="@dimen/heading_text_size"
                style="@style/heading_text_style"
                android:layout_marginStart="15dp"
                android:layout_gravity="center_vertical"/>

            <TextView
                android:layout_weight="1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="|"
                android:gravity="center"
                android:layout_marginLeft="24dp"
                android:textColor="@color/heading_text_color"
                android:textSize="@dimen/heading_text_size"
                android:layout_gravity="center"
                style="@style/heading_text_style" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Collection Fees"
                android:drawablePadding="5dp"
                android:drawableLeft="@drawable/feeparticulars"
                android:textColor="@color/heading_text_color"
                android:textSize="@dimen/title_text_size"
                android:layout_marginRight="16dp"
                android:layout_gravity="center_vertical"
                style="@style/title_text_style"/>
        </LinearLayout>
    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginTop="20dp"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:gravity="center"
        android:visibility="gone"
            android:background="@drawable/edit_text_background2">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="35dp"
                android:padding="5dp"
                android:id="@+id/SelectionSpecific"
                android:entries="@array/select_class" />
        </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:gravity="center"
        android:visibility="gone"
        android:background="@drawable/edit_text_background2">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:padding="5dp"
            android:id="@+id/id"
            android:entries="@array/select_class" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:gravity="center"
        android:visibility="gone"
        android:background="@drawable/edit_text_background2">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:padding="5dp"
            android:id="@+id/schoolId"
            android:entries="@array/select_class" />
    </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginTop="20dp"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:gravity="center"
            android:visibility="gone"

            android:background="@drawable/edit_text_background2">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="35dp"
                android:padding="5dp"
                android:id="@+id/clasName"
                android:entries="@array/select_class" />
    </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginTop="20dp"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:gravity="center"
            android:visibility="gone"
            android:background="@drawable/edit_text_background2">

            <Spinner
                android:layout_width="match_parent"
                android:layout_height="35dp"
                android:padding="5dp"
                android:id="@+id/AvailableDataFeespParticular"
                android:entries="@array/select_class" />

        </LinearLayout>
    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="16dp"
        android:layout_marginTop="10dp">

        <TextView
            android:id="@+id/headerText"
            android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Student Name"
        android:textSize="20sp"
            android:textStyle="bold"
            android:paddingLeft="8dp"
            android:paddingRight="8dp"
            android:layout_gravity="center"
            android:textColor="@color/global_textview_color" />

        <TextView
            android:id="@+id/name"
            android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textStyle="bold"
            android:textSize="20sp"
            android:layout_margin="10dp"
            android:layout_gravity="center"
        android:textColor="@color/global_specific_text_color_blue" />
    </LinearLayout>



    <ScrollView
